{
  "name": "dev-browser",
  "version": "0.0.1",
  "type": "module",
  "exports": {
    ".": "./src/index.ts",
    "./client": "./src/client.ts"
  },
  "scripts": {
    "dev": "bun x tsx --watch src/index.ts",
    "build": "bun x tsup src/index.ts src/client.ts --format esm --dts",
    "start": "bun x tsx src/index.ts"
  },
  "dependencies": {
    "playwright": "^1.49.0"
  },
  "devDependencies": {
    "tsx": "^4.21.0",
    "tsup": "^8.0.0"
  }
}
